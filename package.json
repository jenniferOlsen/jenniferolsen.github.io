{
  "name": "personal-site",
  "description": "Personal site built with Gatsby",
  "version": "1.0.0",
  "author": "Jenny Olsen",
  "main": "index.js",
  "scripts": {
    "develop": "gatsby develop",
    "start": "npm run develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "now-build": "npm run build",
    "deploy": "npm run release && gatsby build && gh-pages -d public -b master",
    "release": "cd public && git add --all && git commit -m \"Release at $(date)\" && git push -f"
  },
  "dependencies": {
    "babel-plugin-styled-components": "^1.9.4",
    "gatsby": "^2.0.33",
    "gatsby-image": "^2.0.26",
    "gatsby-plugin-manifest": "^2.0.12",
    "gatsby-plugin-offline": "^2.0.20",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-sharp": "^2.0.17",
    "gatsby-plugin-styled-components": "^3.0.4",
    "gatsby-plugin-typography": "^2.2.2",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-prismjs": "^3.1.4",
    "gatsby-source-filesystem": "^2.0.16",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.10",
    "prismjs": "^1.15.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-icons": "^3.2.2",
    "react-typography": "^0.16.18",
    "styled-components": "^4.1.2",
    "typography": "^0.16.18",
    "typography-theme-bootstrap": "^0.16.18"
  },
  "devDependencies": {
    "eslint": "^5.12.1",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "^1.2.0",
    "prettier": "^1.14.2"
  }
}
